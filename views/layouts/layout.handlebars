<body>
    <div class="mainwrapper">

        <div class="header">
            <div class="logo">
                <a href="/"><img src="/images/admin_logo_rildo.png" alt="" /></a>
            </div>
            <div class="headerinner">
                <ul class="headmenu">
                    <li>
                        <a id="linkUserNotis" href="/mensagens?view=notifications">
                            <span id="totalUnreadNotifications" class="count">7</span>
                            <span class="head-icon head-message"></span>
                            <span class="headmenu-label">Notificações</span>
                        </a>
                    </li>
                    <li>
                        <a id="linkEntities" href="/gerenciar/entidades">
                            <span id="totalClients" class="count">320</span>
                            <span class="head-icon head-users"></span>
                            <span class="headmenu-label">Clientes</span>
                        </a>
                    </li>
                    <li>
                        <a id="linkEstimates" href="/gerenciar/orcamentos">
                            <span id="totalEstimatesOpened" class="count">110</span>
                            <span class="head-icon head-bar"></span>
                            <span class="headmenu-label">Orçamentos</span>
                        </a>
                    </li>
                    <li>
                        <a id="linkSales" href="/gerenciar/orcamentos/tabid/71/vendas/1/Default.aspx">
                            <span id="totalSales" class="count">740</span>
                            <span class="head-icon head-sales"></span>
                            <span class="headmenu-label">Vendas</span>
                        </a>
                    </li>
                    <li class="right">
                        <div class="userloggedinfo">
                            <img src="/images/photos/thumb1.png" alt="" />
                            <div class="userinfo">
                                <h5>{{user.name}} <small>- {{user.email}}</small></h5>
                                <ul>
                                    <li><a href="/perfil">Editar Perfil</a></li>
                                    <li><a href="/config">Configurações</a></li>
                                    <li><a href="/logout">Logout</a></li>
                                </ul>
                            </div>
                        </div>
                    </li>
                </ul>
                <!--headmenu-->
            </div>
        </div>

        <div class="leftpanel">

            <div class="leftmenu">
                <ul class="nav nav-tabs nav-stacked">
                    <li class="nav-header">Navigation</li>
                    <li class="active"><a href="/"><span class="iconfa-laptop"></span> Início</a></li>
                    <li><a href="/or" target="_blank"><span class="iconfa-briefcase"></span> OR</a></li>
                    <li class="dropdown"><a href=""><span class="iconfa-book"></span> Gerenciar</a>
                        <ul>
                            <li><a href="/admin/categorias">Categorias</a></li>
                            <li><a href="/admin/produtos">Produtos</a></li>
                            <li><a href="/admin/entidades">Entidades</a></li>
                            <li><a href="/admin/colaboradores">Colaboradores</a></li>
                            <li><a href="/admin/davs">DAVs</a></li>
                            <li class="dropdown"><a href=""><span class="iconfa-book"></span> Financeiro</a>
                                <ul>
                                    <li><a href="/admin/categorias">Entradas</a></li>
                                    <li><a href="/admin/produtos">Fluxo</a></li>
                                    <li><a href="/admin/entidades">Caixa</a></li>
                                </ul>
                            </li>
                            <li><a href="/admin/agenda">Agenda</a></li>
                            <li><a href="/admin/relatorios">Relatórios</a></li>
                            <li><a href="/admin/laja">Loja</a></li>
                        </ul>
                    </li>
                    <li><a href="/produtos"><span class="iconfa-briefcase"></span> Produtos</a></li>
                    <li><a href="/quemsomos"><span class="iconfa-briefcase"></span> Quem Somos</a></li>
                    <li><a href="/contatos"><span class="iconfa-briefcase"></span> Contatos</a></li>
                </ul>
            </div>
            <!--leftmenu-->

        </div>
        <!-- leftpanel -->

        <div class="rightpanel">

            <ul class="breadcrumbs">
                <li><a href="/"><i class="iconfa-home"></i></a> <span class="separator"></span></li>
                <li>{{title}}</li>
                <li class="right">
                    <a href="" data-toggle="dropdown" class="dropdown-toggle"><i class="icon-tint"></i> Color Skins</a>
                    <ul class="dropdown-menu pull-right skin-color">
                        <li><a href="default">Default</a></li>
                        <li><a href="navyblue">Navy Blue</a></li>
                        <li><a href="palegreen">Pale Green</a></li>
                        <li><a href="red">Red</a></li>
                        <li><a href="green">Green</a></li>
                        <li><a href="brown">Brown</a></li>
                    </ul>
                </li>
            </ul>

            <div class="pageheader">
                <form action="results.html" method="post" class="searchbar">
                    <input type="text" name="keyword" placeholder="To search type and hit enter..." />
                </form>
                <div class="pageicon"><span class="iconfa-laptop"></span></div>
                <div class="pagetitle">
                    <h5>All Features Summary</h5>
                    <h1>{{title}}</h1>
                </div>
            </div>
            <!--pageheader-->

            <div class="maincontent">
                <div class="maincontentinner">
                    <div class="row-fluid">
                        {{#if success_msg}}
                        <div class="alert alert-success">{{success_msg}}</div>
                        {{/if}} {{#if error_msg}}
                        <div class="alert alert-danger">{{error_msg}}</div>
                        {{/if}} {{#if error}}
                        <div class="alert alert-danger">{{error}}</div>
                        {{/if}} {{{body}}}

                        <!-- body -->
                    </div>
                    <!--row-fluid-->

                    <div class="footer">
                        <div class="footer-left">
